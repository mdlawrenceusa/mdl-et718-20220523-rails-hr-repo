<%= form_for([@resume, @resume.jobs.build]) do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
  
    <p>
    <%= f.label :start_date %><br>
    <%= f.text_field :start_date %>
  </p>
  
  
    <p>
    <%= f.label :end_date %><br>
    <%= f.text_field :end_date %>
  </p>
  

  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>